<ul class="list list-social list-unstyled" role="list" {{ attrs }}>
  {%- if settings.social_twitter_link != blank -%}
    <li data-reveal class="list-social__item">
      <a target="_blank" rel="nofollow noreferrer noopener" href="{{ settings.social_twitter_link }}" class="link list-social__link" >
        {%- render 'icon-twitter' -%}
        <span class="visually-hidden">{{ 'general.social.links.twitter' | t }}</span>
      </a>
    </li>
  {%- endif -%}
